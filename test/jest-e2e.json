{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@common": "<rootDir>/../libs/common/src",
    "schema/schema/(.*)": "<rootDir>/../libs/schema/src/$1",
    "schema/schema": "<rootDir>/../libs/schema/src"
  }
}