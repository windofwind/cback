{
  "name": "cback",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "make-badges": "istanbul-badges-readme",
    "build": "nest build",
    "build:webpack": "rimraf dist && webpack",
    "build:test:webpack": "rimraf dist && webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@nestia/core": "^3.14.0",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.14.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.3",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.19.1",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "dayjs": "^1.11.13",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typia": "^6.10.2"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestia/sdk": "^3.14.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "istanbul-badges-readme": "^1.9.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nestia": "^6.1.0",
    "node-config-webpack": "^1.0.10",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "prisma-markdown": "^1.0.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.6.2",
    "vite-tsconfig-paths": "^5.0.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app(|/.*)$": "<rootDir>/src/$1",
      "^@common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@schema(|/.*)$": "<rootDir>/libs/schema/src/$1",
      "^@storage(|/.*)$": "<rootDir>/libs/storage/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "engines": {
    "node": ">=20.16.0"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}